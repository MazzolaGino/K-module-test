<h1>Mvc Sharing Options</h1>
<form method="post" action="{{referer}}">
	<input type="hidden" name="option_page" value="configuration_group">
	<input type="hidden" name="action" value="update">
	<input type="hidden" id="_wpnonce" name="_wpnonce" value="1d36b93252">
<h2>Twitter Settings</h2>
<table class="form-table" role="presentation">
	<tbody>
		{% for key, value in values %}
		<tr>
			<th scope="row">
				{{ key }}
			</th>
			<td><input type="text" name="{{ key }}" style="width: 100%"
					value="{{ value is not empty ? value : '' }}"></td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<p class="submit">
	<input type="submit" name="submit" id="submit"
		class="button button-primary" value="Enregistrer les modifications">
</p>
</form>
